{
  "name": "typescript-clean-arch-template",
  "version": "1.0.0",
  "main": "index.ts",
  "repository": "git@github.com:dhanielsales/typescript-clean-arch-template.git",
  "author": "Dhaniel Sales <contact@dhanielsales.com>",
  "license": "MIT",
  "keywords": [],
  "type": "module",
  "scripts": {
    "dev:app": "tsx src/index.ts",
    "dev:app-watch": "tsx watch --ignore node_modules src/index.ts",
    "dev:infra": "docker compose up",
    "start": "node dist/index.js",
    "build": "rollup -c rollup.config.js",
    "type-check": "tsc --pretty --noEmit",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:watch": "jest --passWithNoTests --silent --noStackTrace --runInBand --watch",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:staged": "yarn test -o",
    "test:ci": "yarn test --coverage"
  },
  "dependencies": {
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "kafkajs": "^2.2.3",
    "nodemailer": "^6.8.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/express": "^4.17.15",
    "@types/jest": "28.1.2",
    "@types/node": "^18.11.18",
    "@types/nodemailer": "^6.4.7",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "28.1.2",
    "prettier": "^2.2.1",
    "rollup": "^3.10.1",
    "ts-jest": "^28.0.5",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4"
  }
}
